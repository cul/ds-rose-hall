<g class="frond">
  <path class="frond-stem" stroke="black"
    fill="none" {{build-stem this.setStemAndLeaves}}
    />
  <text transform="translate(150, 150)">{{this.svg.width}} ||</text>
  {{#each this.leafs as |leaf|}}
    <path class="frond-leaf" stroke="black" d={{leaf.d}} transform={{leaf.transform}} fill="none"/>
  {{/each}}
</g>



