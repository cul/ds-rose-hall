<g class="frond" opacity="0.8"
  transform={{this.transform}}>
  <path class="frond-stem" stroke={{this.color}}
    stroke-width=".75rem"
    fill="none" {{build-stem this.setStemAndLeaves}}
    />
  {{#each this.leafs as |leaf|}}
    <g class="frond-leaf" transform={{leaf.transform}}>
      <path class="frond-leaf-stem" stroke={{this.color}}
      stroke-width=".4rem"
      fill="none" d={{leaf.stemD}} />
      <path class="frond-leaf" d={{leaf.d}} 
      transform={{leaf.leafRotation}}
      stroke="none"
      fill={{this.color}}/>
    </g>
  {{/each}}
</g>



